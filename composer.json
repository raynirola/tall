{
    "name": "raynirola/tall",
    "description": "Installs latest version of tailwindcss, alpine js, livewire and published files.",
    "keywords": ["laravel", "tall", "raynirola"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ray Nirola",
            "email": "hi@ray.nirola.dev"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/filesystem": "^v8.0|^9.0",
        "illuminate/support": "^v8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ray\\Tall\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ray\\Tall\\TallServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
